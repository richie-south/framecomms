{
  "name": "framecomms",
  "version": "0.0.8",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./subscriber": {
      "require": "./lib/subscriber.js",
      "import": "./lib/subscriber.js",
      "types": "./lib/subscriber.d.ts"
    },
    "./parent": {
      "require": "./lib/parent.js",
      "import": "./lib/parent.js",
      "types": "./lib/parent.d.ts"
    },
    "./constants": {
      "require": "./lib/constants.js",
      "import": "./lib/constants.js",
      "types": "./lib/constants.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "subscriber": [
        "lib/subscriber.d.ts"
      ],
      "parent": [
        "lib/parent.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "test": "vite build && clear && playwright test"
  },
  "keywords": [],
  "author": "richard s√∂derman",
  "description": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "playwright": "^1.52.0",
    "playwright-chromium": "^1.52.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^6.3.5"
  }
}
